<%- include('../partials/header.ejs', { user }) %>

<h1>Search Results for <%= search %></h1>

<form action="/search">
    <label for="search">Search for artwork by name:</label>
    <input id="search" type="text" name="search" />
    <button type="submit">Go</button>
</form>

<% if(result.objectIDs === null) { %>
    <p>whoops no results for <%= search %></p>
<% }else{ %>
<% result.objectIDs.forEach(e => { %>
    <div>
        <p><a href="search/<%= e %>"><%= e %></a></p>
    </div>
<% })} %>